host_cpu=	@host_cpu@

# --- Version -----------------------------------------------------------

GENOM_MAJOR=	1
GENOM_MINOR=	1
GENOM_REV=	B


# --- Programs ----------------------------------------------------------

CPP=		@CPP@
STDINCPP=	@STDINCPP@
PURIFY=		
CC=		@CC@
LTCC=           $(TOPBUILD)/libtool --mode=compile $(CC)
LTLD=           $(TOPBUILD)/libtool --mode=link $(PURIFY) $(CC)
MKDEP=		@MKDEP@
LEX=            @LEX@
YACC=		@YACC@
INSTALL=	$(topsrcdir)/install-sh -c
LTINSTALL=	$(TOPBUILD)/libtool --mode=install $(INSTALL)
LTINSTALL_DATA=	$(TOPBUILD)/libtool --mode=install $(INSTALL) -m 644

SED=		sed
LATEX=		@LATEX@

GENOM=		@GENOM@
TCLSERV=	@TCLSERV@
LIBTCLCLIENT=	lib@GENOM@TclClient$(LIBEXT)


# --- Flags -------------------------------------------------------------

LIBSONLY=	@LIBSONLY@

CPPFLAGS=	@CPPFLAGS@
CFLAGS=		@CFLAGS@
LDFLAGS=	@LDFLAGS@

OSAPI=		@OSAPI@
OS_LIBS=	@OS_LIBS@

PORTLIB_CFLAGS=	@PORTLIB_CFLAGS@
PORTLIB_LDFLAGS=	@PORTLIB_LDFLAGS@

RTAI_INC=	@RTAI_INC@
LNXKERN_INC=	@LNXKERN_INC@

HAS_TCL=	@HAS_TCL@
TCL_CPPFLAGS=	@TCL_CPPFLAGS@
TCL_LDFLAGS=	@TCL_LDFLAGS@ @TCL_LIB_FLAG@ @TCL_LIBS@

ENDIANNESS=	@ENDIANNESS@

LIBTOOL_DEPS=   @LIBTOOL_DEPS@


# --- Paths -------------------------------------------------------------

CANVASDIR=	$(datadir)/genom/$(GENOM_MAJOR).$(GENOM_MINOR)$(GENOM_REV)
