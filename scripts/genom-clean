    autoconf/acinclude.m4  \
    autoconf/config.guess   \
    autoconf/config.posix.mk   \
    autoconf/config.sub           \
    autoconf/configure.end.in   \
    autoconf/install-sh   \
    autoconf/mkinstalldirs \
    autoconf/codels.mk      \
    autoconf/config.mk.in   \
    autoconf/config.rtai.mk    \
    autoconf/configure.begin.in   \
    autoconf/libtool.m4   \
    autoconf/ltmain.sh   \
    autoconf/robots.m4

