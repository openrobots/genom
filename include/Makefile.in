top_srcdir=	 @top_srcdir@
srcdir=		   @srcdir@
top_builddir= @top_builddir@

prefix=		   @prefix@
exec_prefix= @exec_prefix@
includedir=	 @includedir@

INSTALL=  @INSTALL@
VPATH=		$(srcdir)

include $(top_builddir)/config.mk

# -----------------------------------------------------------------------
flist = \
				genom/testLib.h \
				genom/testLibProto.h \
				genom/genomError.h \
				genom/h2endian.h \
				genom/h2endianProto.h \
				genom/moduleEvents.h \
				genom/modules.h \
				genom/posterXMLLib.h \
				genom/printProto.h \
				genom/printScan.h \
				genom/printStateProto.h \
				genom/printXMLProto.h \
				genom/scanProto.h \
				genom/genomModulesLibProto.h

# -----------------------------------------------------------------------

.PHONY: all
all:;

.PHONY: install
install: all
	$(MKINSTALLDIRS) $(includedir)/genom
	@for f in $(flist); do						\
		if ! cmp -s $(srcdir)/$$f $(includedir)/$$f; then \
				$(INSTALL_DATA) $(srcdir)/$$f $(includedir)/$$f;	\
		fi; \
	done

.PHONY: clean
clean:;

distclean: clean
	rm -f Makefile

.PHONY: depend
depend:
